<Window x:Class="w2g.desktop.InputWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:w2g.desktop"
        mc:Ignorable="d"
        Title="InputWindow" Height="100" Width="200">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>

        <TextBox x:Name="UI_Input" Grid.Row="0" Margin="20,10,20,10" TextChanged="UI_Input_TextChanged"/>
        <Button x:Name="UI_Submit" Content="Submit" Grid.Row="1" Margin="20,10,20,10" Click="UI_Submit_Click"/>
    </Grid>
</Window>
